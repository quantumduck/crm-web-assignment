<h1>There are <b><%= Contact.all.length %></b> contacts</h1>
<ul>
  <% Contact.all.each do |contact| %>
    <li>
      <a href= <%= '/contacts/' + contact.id.to_s %> ><b><%= contact.full_name %></b> (<%= contact.email %>)</a>
      <footer>
        <a href="/contacts/<%=contact.id.to_s%>/edit">Edit</a> | <a href="/contacts/<%=contact.id.to_s%>/delete">Delete</a>
      </footer>
    </li>
  <% end %>
</ul>
